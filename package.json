{
  "name": "create-notelink",
  "version": "0.1.2",
  "description": "CLI tool to create NoteLink projects",
  "type": "module",
  "bin": {
    "create-notelink": "./cli/create.ts"
  },
  "files": [
    "cli",
    "template",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "bun build cli/create.ts --outdir dist --target node --format esm",
    "dev": "bun cli/create.ts",
    "test": "bun cli/create.ts test-app",
    "clean:test": "rmdir /s /q test-app 2>nul || rm -rf test-app 2>/dev/null || true",
    "clean:build": "rmdir /s /q dist 2>nul || rm -rf dist 2>/dev/null || true",
    "prebuild": "bun run clean:build",
    "prepublishOnly": "bun run clean:test && bun run build",
    "version": "git add -A",
    "postversion": "git push && git push --tags"
  },
  "keywords": [
    "bun",
    "elysia",
    "notelink",
    "cli",
    "scaffold",
    "generator"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/canvas-tech-horizon/create-notelink.git"
  },
  "bugs": {
    "url": "https://github.com/canvas-tech-horizon/create-notelink/issues"
  },
  "homepage": "https://github.com/canvas-tech-horizon/create-notelink#readme",
  "publishConfig": {
    "access": "public"
  }
}
